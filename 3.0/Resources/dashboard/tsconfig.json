{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@components/*": [
                "components/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@ums-service": [
                "ums-service.ts"
            ],
            "@other-services": [
                "other-services.ts"
            ],
            "@types": [
                "types.ts"
            ],
            "@utils": [
                "utils.ts"
            ],
            "@uxp": [
                "uxp.ts"
            ],
            "@images/*": [
                "images/*"
            ]
        },
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "resolveJsonModule": true,
        "esModuleInterop": true
    }
}